ADD RT_IMP_EXECUTIVE:0 "Imperial Executive"
CN2 RT_IMP_EXECUTIVE:0 "帝国执行官"
ADD HT_IMP_VICE_EXECUTIVE:0 "Imperial Vice Executive"
CN2 HT_IMP_VICE_EXECUTIVE:0 "帝国副执行官"
ADD requires_actor_recepient_not_in_galactic_empire:1 "$TRIGGER_FAIL$§RMembers of the §YGalactic Imperium§! cannot join Federations.§!"
CN2 requires_actor_recepient_not_in_galactic_empire:1 "$TRIGGER_FAIL$§R§Y星海帝国§!的成员不能加入联邦。§!"
