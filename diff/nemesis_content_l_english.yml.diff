ADD resolution_category_custodian_reforms:0 "Custodian Reforms"
CN2 resolution_category_custodian_reforms:0 "监管人改革"
ADD resolution_category_galactic_institutions:0 "Galactic Institutions"
CN2 resolution_category_galactic_institutions:0 "星海机构"
ADD resolution_category_term_limits:0 "Term Limits"
